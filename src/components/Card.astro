---
//src/components/Card.astro
const {name, position, aboutMe, linkedin, github, cvLink, profileImage } = Astro.props;

import '../styles/card.css';
---

<div class="container">
    <div class="card-container">
        <div class="descripcion">
            <h1>{name}</h1>
            <h2>{position}</h2>
            <p>
            Bakso Si Gembul Outlet Bakso Yang Menyediakan Banyak Varian Menu Bakso Kuah Original & Kuah Pedas dan ada juga Menu Mie Kuah.
            </p>
            <br/>
            <p>
            <strong>BAKSO PREMIUM MINIM TEPUNG GRATIS SAYUR</strong>
            </p>
        </div>
        <div class="image">
            <img src={profileImage} alt="">
        </div>
    </div>
</div>